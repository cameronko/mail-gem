{
    "manifest_version": 3,
    "name": "Email Thread Extractor",
    "version": "1.0",
    "description": "Expands Gmail threads and extracts email data.",
    "permissions": ["scripting", "activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
        }
    },
    "content_scripts": [
        {
        "matches": ["https://mail.google.com/*"],
        "js": ["contentScript.js"]
        }
    ]
}